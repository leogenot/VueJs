AUH,CAAC,CAACJ,KAAD,CAAD,CAASQ,aAAT,CAAuBD,CAAC,CAACP,KAAD,CAAxB,CAAV;AAAA,OAAnB;;AACA,UAAId,IAAG,GAAI,KAAKE,KAAL,CAAWqB,MAAX,CAAkBN,UAAlB,CAAX;AACAjB,UAAG,GAAIA,IAAI,CAACwB,IAAL,CAAUJ,UAAV,CAAP;AACA,UAAIJ,QAAJ,EAAchB,IAAG,GAAIA,IAAI,CAACyB,OAAL,EAAP;AACd,aAAOzB,IAAP;AACF;AAbU,GA9BC;AA6Cb0B,SAAO,EAAE;AACPf,iBADO,2BACS;AACd,UAAMgB,OAAM,GAAIlB,MAAM,CAACkB,OAAvB;AACA,UAAMC,OAAM,GAAIC,QAAQ,CAACC,eAAT,CAAyBC,YAAzC;AACA,UAAMC,UAAS,GAAIH,QAAQ,CAACC,eAAT,CAAyBG,YAA5C;AACA,UAAMC,YAAW,GAAIN,OAAM,GAAID,OAAV,IAAqBK,UAA1C;AACA,aAAOE,YAAW,IAAKF,UAAS,GAAIJ,OAApC;AACD,KAPM;AAQPrB,WARO,qBAQG;AAAA;;AACRV,WAAK,CAACsC,GAAN,CAAU,yCAAV,EAAqDC,IAArD,CAA0D,UAACC,QAAD,EAAc;AACtE,YAAIC,GAAE,GAAID,QAAQ,CAACrC,IAAT,CAAc,CAAd,CAAV;AACA,YAAIuC,OAAM,GAAI;AACZzC,cAAI,EAAEwC,GAAG,CAACxC,IADE;AAEZ0C,aAAG,EAAEF,GAAG,CAACE,GAFG;AAGZC,cAAI,EAAEH,GAAG,CAACI,WAHE;AAIZC,aAAG,EAAEL,GAAG,CAACM,SAJG;AAKZC,cAAI,EAAEP,GAAG,CAACQ,YALE;AAMZC,iBAAO,EAAET,GAAG,CAACS,OAND;AAOZC,cAAI,EAAEV,GAAG,CAACW;AAPE,SAAd;;AASA,cAAI,CAAC/C,KAAL,CAAWgD,IAAX,CAAgBX,OAAhB;;AACC,YAAI,MAAI,CAAC5B,aAAL,EAAJ,EAA0B;AACzB,gBAAI,CAACJ,OAAL;AACF;AACD,OAfD;AAiBD;AA1BM;AA7CI,CAAf","names":["BeerCard","axios","name","components","data","bottom","beers","search","beersSortType","watch","newValue","addBeer","created","window","addEventListener","bottomVisible","computed","beersOrganizationData","field","includes","reversed","filterFunc","a","toLowerCase","comparator","b","localeCompare","filter","sort","reverse","methods","scrollY","visible","document","documentElement","clientHeight","pageHeight","scrollHeight","bottomOfPage","get","then","response","api","apiInfo","abv","desc","description","img","image_url","tips","brewers_tips","tagline","food","food_pairing","push"],"sourceRoot":"","sources":["/Users/leogenot/Desktop/VueJs/src/App.vue"],"sourcesContent":["<template>\n  <div id=\"app\">\n    <section>\n      <div class=\"list-options\">\n        <input\n          type=\"text\"\n          v-model=\"search\"\n          placeholder=\"Type a beer name\"\n        />\n        <label for=\"beer-sort\">Sort by : </label>\n        <select v-model=\"beersSortType\" id=\"beer-sort\">\n          <option value=\"AZName\">Sort from A to Z</option>\n          <option value=\"ZAName\">Sort from Z to A</option>\n        </select>\n      </div>\n      <h1>List of all beers</h1>\n      <div v-if=\"!beers.length\" class=\"loading\">Loading...</div>\n      <BeerCard\n        v-for=\"beer in beersOrganizationData\"\n        :key=\"beer\"\n        :name=\"beer.name\"\n        :img=\"beer.img\"\n        :abv=\"beer.abv\"\n        :tagline=\"beer.tagline\"\n        :desc=\"beer.desc\"\n        :tips=\"beer.tips\"\n        :food=\"beer.food\"\n      />\n    </section>\n  </div>\n</template>\n\n<script>\nimport BeerCard from \"./components/BeerCard.vue\";\nimport axios from \"axios\";\nexport default {\n  name: \"BeerGallery\",\n  components: {\n    BeerCard,\n  },\n  data() {\n    return {\n      bottom: false,\n      beers: [],\n      search: \"\",\n      beersSortType: \"AZName\",\n    };\n  },\n  watch: {\n    bottom(newValue) {\n      if (newValue) {\n        this.addBeer();\n      }\n    },\n  }, \n  created() {\n     window.addEventListener(\"scroll\", () => {\n      this.bottom = this.bottomVisible();\n    }); \n    this.addBeer();\n    this.addBeer();\n    this.addBeer();\n    this.addBeer();\n    this.addBeer();\n  },\n    computed: {\n    beersOrganizationData() {\n      const field = [\"AZName\", \"ZAName\"].includes(this.beersSortType)\n        ? \"name\"\n        : \"abv\";\n      const reversed = [\"ZAName\"].includes(this.beersSortType);\n      const filterFunc = (a) =>\n        a.name.toLowerCase().includes(this.search.toLowerCase());\n      const comparator = (a, b) => a[field].localeCompare(b[field]);\n      let data = this.beers.filter(filterFunc);\n      data = data.sort(comparator);\n      if (reversed) data = data.reverse();\n      return data;\n    }\n  },\n  methods: {\n    bottomVisible() {\n      const scrollY = window.scrollY;\n      const visible = document.documentElement.clientHeight;\n      const pageHeight = document.documentElement.scrollHeight;\n      const bottomOfPage = visible + scrollY >= pageHeight;\n      return bottomOfPage || pageHeight < visible;\n    },\n    addBeer() {\n      axios.get(\"https://api.punkapi.com/v2/beers/random\").then((response) => {\n        let api = response.data[0];\n        let apiInfo = {\n          name: api.name,\n          abv: api.abv,\n          desc: api.description,\n          img: api.image_url,\n          tips: api.brewers_tips,\n          tagline: api.tagline,\n          food: api.food_pairing,\n        };\n        this.beers.push(apiInfo);\n         if (this.bottomVisible()) {\n          this.addBeer();\n        } \n      });\n      \n    },\n    \n  },\n};\n</script>\n\n<style></style>\n"]}]}